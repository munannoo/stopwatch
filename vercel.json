{
    "version": 2,
    "builds": [
        {
            "src": "build/index.html",
            "use": "@vercel/static-build",
            "config": {
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/$1"
        }
    ]
}